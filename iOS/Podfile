# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'HoloDex' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HoloDex
  pod 'ObjectMapper', :inhibit_warnings => true
  pod 'Alamofire'
  pod 'AlamofireObjectMapper', :inhibit_warnings => true
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'ReSwift'
  pod 'ReSwiftRouter'
  pod 'PinLayout'
  pod 'FlexLayout'
end
